{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "cli": {
    "packageManager": "npm"
  },
  "newProjectRoot": "projects",
  "projects": {
    "team-adhs-website": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "Website",
      "sourceRoot": "Website/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/team-adhs-website",
            "index": "Website/src/index.html",
            "main": "Website/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "Website/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "Website/src/favicon.ico",
              "Website/src/assets"
            ],
            "styles": [
              "Website/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "1000kb",
                  "maximumError": "3mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "10kb",
                  "maximumError": "20kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "buildTarget": "team-adhs-website:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "team-adhs-website:build:production"
            },
            "development": {
              "buildTarget": "team-adhs-website:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "team-adhs-website:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "Website/tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "Website/src/favicon.ico",
              "Website/src/assets"
            ],
            "styles": [
              "Website/src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    }
  }
}
